<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <TextBlock Width="110" Height="40" Text="TextBlock" />
        <TextBox Width="110" Height="35" Text="TextBox"/>
        <ProgressBar Width="110" Height="20" Value="50" Minimum="0" Maximum="100"/>
        <Slider Width="150" Height="30" Value="10" Maximum="100" />
        <PasswordBox Password="Secret" Width="150" Height="30" />
    </StackPanel>
</Window>
